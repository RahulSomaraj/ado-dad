{
  "name": "new",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:all": "node test/test-runner.js",
    "test:all:html": "node test/enhanced-test-runner.js",
    "test:auth": "jest --config ./test/jest-e2e.json --testPathPattern=auth.e2e-spec.ts",
    "test:users": "jest --config ./test/jest-e2e.json --testPathPattern=users.e2e-spec.ts",
    "test:vehicle-inventory": "jest --config ./test/jest-e2e.json --testPathPattern=vehicle-inventory.e2e-spec.ts",
    "test:ads": "jest --config ./test/jest-e2e.json --testPathPattern=ads.e2e-spec.ts",
    "test:ads-minimal": "node test/run-ads-minimal-tests.js",
    "test:report": "npm run test:all && open coverage/test-execution-report.json",
    "test:report:html": "npm run test:all:html && open reports/comprehensive-test-report.html",
    "test:coverage:html": "npm run test:cov && open coverage/lcov-report/index.html",
    "test:demo:html": "node test/demo-html-report.js",
    "test:performance": "node test/performance-test.js",
    "test:performance:open": "node test/performance-test.js && open test/performance-report.html",
    "seed": "ts-node src/vehicle-inventory/seed/seed-script.ts",
    "seed:ads": "ts-node src/ads/seed/run-seed.ts",
    "seed:manufacturers": "ts-node src/vehicle-inventory/seed/run-manufacturer-seed.ts",
    "seed:vehicle-models": "ts-node src/vehicle-inventory/seed/seed-vehicle-models-standalone.ts",
    "seed:vehicle-variants": "ts-node src/vehicle-inventory/seed/run-vehicle-variant-seed.ts",
    "debug:manufacturers": "ts-node src/vehicle-inventory/seed/debug-manufacturers.ts",
    "debug:vehicle-models": "ts-node src/vehicle-inventory/seed/debug-vehicle-models.ts",
    "debug:vehicle-variants": "ts-node src/vehicle-inventory/seed/debug-vehicle-variants.ts",
    "clean:variants": "ts-node src/vehicle-inventory/seed/clean-and-reseed-variants.ts",
    "add:superadmin": "ts-node src/users/seed/add-super-admin.ts",
    "fix:commercial-vehicle-data": "ts-node src/ads/scripts/fix-commercial-vehicle-data.ts",
    "validate:data": "ts-node -e \"import('./src/ads/scripts/fix-commercial-vehicle-data.ts').then(m => m.fixCommercialVehicleData())\"",
    "data:consistency-check": "curl -X GET \"http://localhost:5001/ads/validation/consistency\" -H \"Authorization: Bearer YOUR_TOKEN\"",
    "data:cleanup": "curl -X POST \"http://localhost:5001/ads/validation/cleanup\" -H \"Authorization: Bearer YOUR_TOKEN\""
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.744.0",
    "@aws-sdk/s3-request-presigner": "^3.744.0",
    "@nestjs/common": "^11.1.5",
    "@nestjs/config": "^4.0.0",
    "@nestjs/core": "^11.1.1",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/mongoose": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.5",
    "@nestjs/platform-socket.io": "^11.0.10",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/typeorm": "^11.0.0",
    "@nestjs/websockets": "^11.0.10",
    "@types/moment": "^2.13.0",
    "@types/morgan": "^1.9.9",
    "@types/passport": "^1.0.17",
    "@types/socket.io": "^3.0.2",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "chalk": "^4.1.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "express-rate-limit": "^7.5.0",
    "form-data": "^4.0.3",
    "helmet": "^8.0.0",
    "moment": "^2.30.1",
    "mongoose": "^8.9.5",
    "mongoose-delete": "^1.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "multer-s3": "^3.0.1",
    "nodemailer": "^6.10.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.13.1",
    "redis": "^4.6.13",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "socket.io": "^4.7.3",
    "socket.io-client": "^4.8.1",
    "swagger-ui-express": "^5.0.1",
    "typeorm": "^0.3.25"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.3",
    "@types/express-rate-limit": "^5.1.3",
    "@types/helmet": "^0.0.48",
    "@types/jest": "^29.5.14",
    "@types/mongoose-delete": "^1.0.6",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.15.21",
    "@types/passport": "^1.0.17",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/redis": "^4.0.11",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^10.0.0",
    "axios": "^1.10.0",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^15.14.0",
    "jest": "^29.7.0",
    "jest-html-reporter": "^4.3.0",
    "jest-junit": "^16.0.0",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.20.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "reporters": [
      "default",
      [
        "jest-html-reporter",
        {
          "pageTitle": "Unit Test Report",
          "outputPath": "./reports/unit-test-report.html",
          "includeFailureMsg": true,
          "includeConsoleLog": true,
          "includeStackTrace": true
        }
      ],
      [
        "jest-junit",
        {
          "outputDirectory": "./reports",
          "outputName": "unit-test-results.xml",
          "classNameTemplate": "{classname}",
          "titleTemplate": "{title}",
          "ancestorSeparator": " â€º ",
          "usePathForSuiteName": true
        }
      ]
    ]
  }
}
